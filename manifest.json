{
  "manifest_version": 2,
  "name": "Wordle",
  "version": "1.0",
  "icon": {},
  "browser_action": {
    "default_popup": "index.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens popup.html"
    }
  },
  "permissions": ["tabs"]
}
